<?xml version='1.0'?>

<robot name="trash_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find semesterprosjekt)/urdf/common_macros.xacro" />
  <xacro:include filename="$(find semesterprosjekt)/urdf/trash_arm.urdf.gazebo" />
  
  <gazebo>
    <static>true</static>
  </gazebo>


  <!-- MOBILE BASE -->
  <!--Base mÃ¥ byttast ut til den respektiv basen armen skal festast til!!!-->
  <!-- ==================================== -->
  <xacro:link_box link_name="mobile_body_link" length="0.65" width="0.4" height="0.2" mesh_name="package://semesterprosjekt/mesh/base_mesh.stl">
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_box>

  <!--############################### -->
  <!-- ROBOTIC ARM -->
  <!--############################### -->

  <!-- ARM BASE -->
  <!-- ==================================== -->
  <joint type="revolute" name="arm_base_joint">
    <origin xyz="0 0 0.10" rpy="0 0 0"/>
    <child link="arm_base_link"/>
    <parent link="mobile_body_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi}" upper="${pi}"/>
  </joint>

  <xacro:link_cylinder link_name="arm_base_link" radius="${0.20/2}" length="0.10" density="1" mesh_name="package://semesterprosjekt/mesh/Trash_Arm_F1_Fixed04.stl">
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="0 0 ${pi/2}"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <!-- LINK 1 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_1_joint">
    <origin xyz="0 0 0.10" rpy="-${pi/2} 0 0"/>
    <child link="link_1_link"/>
    <parent link="arm_base_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi}" upper="${pi}"/>
  </joint>

  <xacro:link_cylinder link_name="link_1_link" radius="${0.10/2}" length="0.10" density="1" >
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="-${pi} 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <!-- LINK 2 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_2_joint">
    <origin xyz="0 -${0.35/2} 0" rpy="${pi/2} 0 0"/>
    <child link="link_2_link"/>
    <parent link="link_1_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi/2}" upper="${pi/2}"/>
  </joint>

  <xacro:link_cylinder link_name="link_2_link" radius="${0.10/2}" length="0.35" density="1" mesh_name="package://semesterprosjekt/mesh/Trash_Arm_F2.stl">
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="${pi/2} 0 0 "/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <!-- LINK 3 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_3_joint">
    <origin xyz="0 0 ${0.35/2}" rpy="0 0 0"/>
    <child link="link_3_link"/>
    <parent link="link_2_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi/2}" upper="${pi/2}"/>
  </joint>

  <xacro:link_cylinder link_name="link_3_link" radius="${0.12/2}" length="0.05" density="1" >
    <!-- origin visual -->
    <origin xyz="0 0 ${0.05/2}" rpy="0 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 ${0.05/2}" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 ${0.05/2}" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <!-- LINK 4 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_4_joint">
    <origin xyz="0 0 ${0.10}" rpy="-${pi/2} 0 0"/>
    <child link="link_4_link"/>
    <parent link="link_3_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi}" upper="${pi}"/>
  </joint>

  <xacro:link_cylinder link_name="link_4_link" radius="${0.10/2}" length="0.10" density="1" >
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <!-- LINK 5 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_5_joint">
    <origin xyz="0 -${0.15/2} 0" rpy="${pi/2} 0 0"/>
    <child link="link_5_link"/>
    <parent link="link_4_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi/2}" upper="${pi/2}"/>
  </joint>

  <xacro:link_cylinder link_name="link_5_link" radius="${0.10/2}" length="0.15" density="1" >
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <!-- LINK 6 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_6_joint">
    <origin xyz="0 0 ${0.15/2}" rpy="0 0 0"/>
    <child link="link_6_link"/>
    <parent link="link_5_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi/2}" upper="${pi/2}"/>
  </joint>

  <xacro:link_cylinder link_name="link_6_link" radius="${0.12/2}" length="0.05" density="1" >
    <!-- origin visual -->
    <origin xyz="0 0 ${0.05/2}" rpy="0 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 ${0.05/2}" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 ${0.05/2}" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <!-- LINK 7 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_7_joint">
    <origin xyz="0 0 0.10" rpy="-${pi/2} 0 0"/>
    <child link="link_7_link"/>
    <parent link="link_6_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi}" upper="${pi}"/>
  </joint>

  <xacro:link_cylinder link_name="link_7_link" radius="${0.10/2}" length="0.10" density="1" >
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>


  <!--############################### -->
  <!-- TRANSMISSIONS -->
  <!--############################### -->

  <!-- TRANSMISSION ARM_BASE-->
  <!-- ==================================== -->
  <transmission name="arm_base_tran">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="arm_base_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="arm_base_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- TRANSMISSION JOINT_1-->
  <!-- ==================================== -->
  <transmission name="joint_1_tran">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="link_1_joint">
       <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="joint_1_motor">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

    <!-- TRANSMISSION JOINT_2-->
    <!-- ==================================== -->
    <transmission name="joint_2_tran">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="link_2_joint">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
        <actuator name="joint_2_motor">
          <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
          <mechanicalReduction>1</mechanicalReduction>
        </actuator>
      </transmission>

    <!-- TRANSMISSION JOINT_3-->
    <!-- ==================================== -->
    <transmission name="joint_3_tran">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="link_3_joint">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="joint_3_motor">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

    <!-- TRANSMISSION JOINT_4-->
    <!-- ==================================== -->
    <transmission name="joint_4_tran">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="link_4_joint">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="joint_4_motor">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

    <!-- TRANSMISSION JOINT_5-->
    <!-- ==================================== -->
    <transmission name="joint_5_tran">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="link_5_joint">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="joint_5_motor">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

    <!-- TRANSMISSION JOINT_6-->
    <!-- ==================================== -->
    <transmission name="joint_6_tran">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="link_6_joint">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
        <actuator name="joint_6_motor">
          <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
          <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

    <!-- TRANSMISSION JOINT_7-->
    <!-- ==================================== -->
    <transmission name="joint_7_tran">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="link_7_joint">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="joint_7_motor">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

</robot>
