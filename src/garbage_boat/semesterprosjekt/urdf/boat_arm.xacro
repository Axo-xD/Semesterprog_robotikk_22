<?xml version="1.0"?>

<robot name="boat_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find semesterprosjekt)/urdf/common_macros.xacro" />
  <xacro:include filename="$(find semesterprosjekt)/urdf/boat_arm.urdf.gazebo" />

  <!-- <gazebo>
    <static>true</static>
  </gazebo> -->

 <!-- <xacro:property name="density" value="1" /> -->

  <material name="COLOR">
   <color rgba="1 1 1 1"/>
 </material>
  <link name='base_link'>
    <pose>0 0 0.1 0 0 0</pose>
    <visual name='chassis_visual'>
      <origin xyz="0 0 -0.1" rpy="0 0 ${pi/2}"/>
      <geometry>
        <mesh filename="package://semesterprosjekt/mesh/RobotBatPOrsjekt_v4_v5.stl" scale="0.001 0.001 0.001"/>
       <!-- <box size="1 0.3 0.1"/> -->
      </geometry>
      <material name="COLOR"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.07" rpy="0 0 0"/>
      <geometry>
        <!-- <box size="1 0.4 0.1"/> -->
        <box size="1 0.4 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="0.1075" ixy="0.0" ixz="0.0" iyy="10.0008" iyz="0.0" izz="10.0075"/>
    </inertial>
</link>

  <link name='left_thruster'>
    <visual>
      <geometry>
       <cylinder length="0.07" radius="0.03"/>
     </geometry>
     <!-- <origin rpy="1.5707963 0 0" xyz="0.15 0.465 -0.03"/> -->
     <origin rpy="${pi/2} 0 ${pi/2}" xyz="-0.465 0.15 -0.03"/>
   </visual>
  </link>

  <link name='right_thruster'>
    <visual>
      <geometry>
       <cylinder length="0.07" radius="0.03"/>
     </geometry>
     <!-- <origin rpy="1.5707963 0 0" xyz="-0.15 0.465 -0.03"/> -->
     <origin rpy="${pi/2} 0 ${pi/2}" xyz="-0.465 -0.15 -0.03"/>
   </visual>
  </link>

  <joint name="base_to_left_thruster" type="fixed">
    <parent link="base_link"/>
    <child link="left_thruster"/>
  </joint>

  <joint name="base_to_right_thruster" type="fixed">
    <parent link="base_link"/>
    <child link="right_thruster"/>
  </joint>

  <joint name="left_front_wheel_j" type="continuous">
    <origin xyz="0.20 0.20 0" rpy="${-pi/2} 0 0"/>
    <parent link="base_link"/>
    <child link="lf_wheel"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
  </joint>

  <xacro:link_wheel link_name="lf_wheel">
    <!-- collison -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- inertia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_wheel>

  <joint name="right_front_wheel_j" type="continuous">
    <origin xyz="0.20 -0.20 0" rpy="${-pi/2} 0 0"/>
    <parent link="base_link"/>
    <child link="rf_wheel"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
  </joint>

  <xacro:link_wheel link_name="rf_wheel">
    <!-- collison -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- inertia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_wheel>


  <joint name="left_rear_wheel_j" type="continuous">
    <origin xyz="-0.20 0.20 0" rpy="${-pi/2} 0 0"/>
    <parent link="base_link"/>
    <child link="lr_wheel"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
  </joint>

  <xacro:link_wheel link_name="lr_wheel">
    <!-- collison -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- inertia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_wheel>

  <joint name="right_rear_wheel_j" type="continuous">
    <origin xyz="-0.20 -0.20 0" rpy="${-pi/2} 0 0"/>
    <parent link="base_link"/>
    <child link="rr_wheel"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
  </joint>

  <xacro:link_wheel link_name="rr_wheel">
    <!-- collison -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- inertia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_wheel>

  <!--############################### -->
  <!-- ROBOTIC ARM -->
  <!--############################### -->

  <!-- ARM BASE -->
  <!-- ==================================== -->
  <joint type="revolute" name="arm_base_joint">
    <origin xyz="0.05 0 0.18" rpy="0 0 0"/>
    <child link="arm_base_link"/>
    <parent link="base_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi}" upper="${pi}"/>
  </joint>

  <xacro:link_cylinder link_name="arm_base_link" radius="${0.20/2}" length="0.10" density="1" mesh_name="package://semesterprosjekt/mesh/Trash_Arm_F1_Fixed04.stl">
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="0 0 ${pi/2}"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <transmission name="arm_base_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="arm_base_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="arm_base_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- LINK 1 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_1_joint">
    <origin xyz="0 0 0.10" rpy="-${pi/2} 0 0"/>
    <child link="link_1_link"/>
    <parent link="arm_base_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi}" upper="${pi}"/>
  </joint>

  <xacro:link_cylinder link_name="link_1_link" radius="${0.10/2}" length="0.10" density="1"  >
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="-${pi} 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <transmission name="joint_1_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="link_1_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="joint_1_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- LINK 2 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_2_joint">
    <origin xyz="0 -${0.40/2} 0" rpy="${pi/2} 0 0"/>
    <child link="link_2_link"/>
    <parent link="link_1_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi/2}" upper="${pi/2}"/>
  </joint>

  <xacro:link_cylinder link_name="link_2_link" radius="${0.10/2}" length="0.40" density="1">
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="0 0 0 "/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <transmission name="joint_2_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="link_2_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="joint_2_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>


  <!-- LINK 3 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_3_joint">
    <origin xyz="0 0 ${0.40/2}" rpy="-${pi/2} 0 0"/>
    <child link="link_3_link"/>
    <parent link="link_2_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi}" upper="${pi}"/>
  </joint>

  <xacro:link_cylinder link_name="link_3_link" radius="${0.10/2}" length="0.10" density="1" >
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <transmission name="joint_3_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="link_3_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="joint_3_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- LINK 4 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_4_joint">
    <origin xyz="0 -${0.20/2} 0" rpy="${pi/2} 0 0"/>
    <child link="link_4_link"/>
    <parent link="link_3_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi/2}" upper="${pi/2}"/>
  </joint>

  <xacro:link_cylinder link_name="link_4_link" radius="${0.10/2}" length="0.20" density="1" >
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <transmission name="joint_4_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="link_4_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="joint_4_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- LINK 5 -->
  <!-- ==================================== -->
  <joint type="revolute" name="link_5_joint">
    <origin xyz="0 0 0.10" rpy="-${pi/2} 0 0"/>
    <child link="link_5_link"/>
    <parent link="link_4_link"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="100" velocity="100" lower="-${pi}" upper="${pi}"/>
  </joint>

  <xacro:link_cylinder link_name="link_5_link" radius="${0.10/2}" length="0.10" density="1" >
    <!-- origin visual -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin collision -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- origin interia -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:link_cylinder>

  <transmission name="joint_5_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="link_5_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="joint_5_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>


</robot>
